{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Iniciar Metabuscador",
            "request": "launch",
            "mainClass": "jade.Boot",
            "cwd": "${workspaceFolder}/ProyectoJADE",            "args": [
                "-gui",
                "-agents",
                "agenteInterfaz:es.upm.metabuscador.agentes.AgenteInterfaz;agenteCoordinador:es.upm.metabuscador.agentes.AgenteCoordinador;agenteSerpStack:es.upm.metabuscador.agentes.AgenteBuscador"
            ],
            "vmArgs": "-Dfile.encoding=UTF-8",            "classPaths": [
                "${workspaceFolder}/ProyectoJADE/bin",
                "${workspaceFolder}/ProyectoJADE/lib/jade.jar",
                "${workspaceFolder}/ProyectoJADE/lib/commons-codec-1.3.jar",
                "${workspaceFolder}/ProyectoJADE/lib/json-20140107.jar"
            ],
            "console": "integratedTerminal"
        }
    ]
}